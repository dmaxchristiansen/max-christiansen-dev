{
  "name": "max-christiansen-dev",
  "private": false,
  "description": "Portfolio site for developer Max Christiansen",
  "version": "0.1.0",
  "author": "Daniel Max Christiansen <dmaxchristiansen@gmail.com>",
  "dependencies": {
    "@mux/mux-video-react": "^0.5.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "gatsby": "^4.24.4",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-styled-components": "^5.24.0",
    "gatsby-plugin-webfonts": "^2.2.2",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "^18.7.23",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "enforce-branch-name": "^1.1.2",
    "eslint": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-tsconfig-paths": "^1.0.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "never"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "referenceActions": null,
        "issuePrefixes": [
          "MCD-"
        ]
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": "16.17.1",
    "npm": "8.15.0"
  }
}

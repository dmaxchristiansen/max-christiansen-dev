{
  "name": "frontend-showcase",
  "private": false,
  "description": "A showcase of frontend React components built in Gatsby",
  "version": "0.1.0",
  "author": "Daniel Max Christiansen <dmaxchristiansen@gmail.com>",
  "dependencies": {
    "gatsby": "^4.24.0",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.26",
    "enforce-branch-name": "^1.1.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "commitlint": {
    "rules": {
      "references-empty": [
        2,
        "never"
      ]
    },
    "parserPreset": {
      "parserOpts": {
        "referenceActions": null,
        "issuePrefixes": [
          "FS-"
        ]
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "16.15.0",
    "npm": "8.5.5"
  }
}
